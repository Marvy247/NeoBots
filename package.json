{
  "name": "ai-agent-marketplace",
  "version": "1.0.0",
  "description": "Autonomous AI Research Agent Marketplace on PinionOS",
  "type": "module",
  "scripts": {
    "dev:marketplace": "tsx src/marketplace/server.ts",
    "dev:researcher": "tsx src/agents/researcher.ts",
    "dev:analyzer": "tsx src/agents/analyzer.ts",
    "dev:summarizer": "tsx src/agents/summarizer.ts",
    "dev:all": "concurrently \"npm run dev:marketplace\" \"npm run dev:researcher\" \"npm run dev:analyzer\" \"npm run dev:summarizer\"",
    "dev:frontend": "cd frontend && npm run dev",
    "demo": "tsx demo-simple.ts",
    "build": "tsc",
    "start": "node dist/marketplace/server.js"
  },
  "dependencies": {
    "pinion-os": "latest",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "concurrently": "^8.2.2"
  }
}
